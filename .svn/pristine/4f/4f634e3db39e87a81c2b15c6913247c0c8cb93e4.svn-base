package com.gmail.sungmin0511a.hVariable;

public class HInteger {
	int value;
	
	/** @param value */
	public HInteger(int value) {
		super();
		this.value = value;
	}
	
	/** @return the value */
	public int getValue() {
		return value;
	}
	
	/** @param value
	 *        the value to set */
	public void setValue(int value) {
		this.value = value;
	}
}
